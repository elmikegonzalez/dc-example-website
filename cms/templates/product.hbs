<div class="amp-ca-product-image-holder">
    <picture class="amp-ca-product-image">
        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=1600{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=3200{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(min-width: 1280px)" type="image/webp">
        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=1600{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=3200{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(min-width: 1280px)">

        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=1280{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=2560{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(min-width: 1024px)" type="image/webp">
        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=1280{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=2560{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(min-width: 1024px)">

        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=1024{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=2048{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(min-width: 768px)" type="image/webp">
        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=1024{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=2048{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(min-width: 768px)">

        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=768{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=1536{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(max-width: 768px)" type="image/webp">
        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=768{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=1536{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(max-width: 768px)">
        <img src="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=1600{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true" srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=1600{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=3200{{#if poi}}&$mrhi_poi${{/if}}&upscale=false&fmt.jpeg.interlaced=true 2x,
